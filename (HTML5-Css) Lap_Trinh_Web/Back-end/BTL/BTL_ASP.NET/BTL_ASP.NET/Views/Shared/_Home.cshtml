<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <title>@ViewBag.Title</title>

    <!-- Favicon icon
        TempData['title']

    -->
    <link rel="icon" type="image/png" sizes="16x16" href="~/Content/images/favicon.png">
    <link rel="stylesheet" href="~/Content/vendor/owl-carousel/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Content/vendor/owl-carousel/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/Content/vendor/jqvmap/css/jqvmap.min.css">
    <link rel="stylesheet" href="~/Content/css/style.css">
    <link rel="stylesheet" href="~/Content/icons/font-awesome-old/css/font-awesome.min.css">
    <!-- css Nhập xuất kho -->
    <link rel="stylesheet" type="text/css" href="~/Content/css/styleNhapXuatKho.css">
    <script src="https://cdn.ckeditor.com/ckeditor5/11.0.1/classic/ckeditor.js"></script>
    <!-- jsGrid -->
    @*<link href="~/Content/assets/css/lib/themify-icons.css" rel="stylesheet">
        <link href="~/Content/assets/css/lib/jsgrid/jsgrid-theme.min.css" rel="stylesheet" />
        <link href="~/Content/assets/css/lib/jsgrid/jsgrid.min.css" type="text/css" rel="stylesheet" />
        <link href="~/Content/assets/css/lib/bootstrap.min.css" rel="stylesheet">
        <link href="~/Content/assets/css/style.css" rel="stylesheet">*@

</head>

<body style="background-image: url('../../Content/images/White4.jpg');
        background-repeat: no-repeat;
        background-attachment: fixed;
        background-size: cover;
">

    <!--*******************
        Preloader start
    ********************-->
    <div id="preloader">
        <div class="sk-three-bounce">
            <div class="sk-child sk-bounce1"></div>
            <div class="sk-child sk-bounce2"></div>
            <div class="sk-child sk-bounce3"></div>
        </div>
    </div>
    <!--*******************
        Preloader end
    ********************-->
    <!--**********************************
        Main wrapper start
    ***********************************-->
    <div id="main-wrapper">

        <!--**********************************
            Nav header start
        ***********************************-->
        <div class="nav-header">
            <a href="@Url.Action("Index", "Home")" class="brand-logo">
                <img class="logo-abbr" src="~/Content/images/logo.png" alt="">
        <img class="logo-compact" src="~/Content/images/logo-text.png" alt="">
        <img class="brand-title" src="~/Content/images/logo-text.png" alt="">
                @*<h1 class="text-light"> Trang chủ</h1>*@
            </a>

            <div class="nav-control">
                <div class="hamburger">
                    <span class="line"></span><span class="line"></span><span class="line"></span>
                </div>
            </div>
        </div>
        <!--**********************************
            Nav header end
        ***********************************-->
        <!--**********************************
            Header start
        ***********************************-->
        @Html.Partial("_Header");
        <!--**********************************
            Header end ti-comment-alt
        ***********************************-->
        <!--**********************************
            Sidebar start
        ***********************************-->
        @Html.Partial("_SideBar");
        <!--**********************************
            Sidebar end
        ***********************************-->
        <!--**********************************
            Content body start
        ***********************************-->

        <div class="content-body">
            <!-- row -->
            <div class="container-fluid">
                @RenderBody()
            </div>
        </div>

        <!--**********************************
            Content body end
        ***********************************-->
        <!--**********************************
            Footer start
        ***********************************-->
        @Html.Partial("_Footer");
        <!--**********************************
            Footer end
        ***********************************-->
        <!--**********************************
           Support ticket button start
        ***********************************-->
        <!--**********************************
           Support ticket button end
        ***********************************-->
    </div>
    <!--**********************************
        Main wrapper end
    ***********************************-->
    <!--**********************************
        Scripts
    ***********************************-->
    <!-- Required vendors -->

    <script src="~/Content/vendor/global/global.min.js"></script>
    <script src="~/Scripts/js/quixnav-init.js"></script>
    <script src="~/Scripts/js/custom.min.js"></script>

    <!-- Vectormap
        <script src="~/Content/vendor/morris/morris.min.js"></script>
        -->
    <script src="~/Content/vendor/raphael/raphael.min.js"></script>
    <script src="~/Content/vendor/morris/morris.min.js"></script>
    <script src="~/Content/vendor/circle-progress/circle-progress.min.js"></script>
    <script src="~/Content/vendor/chart.js/Chart.bundle.min.js"></script>
    <script src="~/Content/vendor/gaugeJS/dist/gauge.min.js"></script>

    <!--  flot-chart js -->
    <script src="~/Content/vendor/flot/jquery.flot.js"></script>
    <script src="~/Content/vendor/flot/jquery.flot.resize.js"></script>

    <!-- Owl Carousel -->
    <script src="~/Content/vendor/owl-carousel/js/owl.carousel.min.js"></script>

    <!-- Counter Up -->
    <script src="~/Content/vendor/jqvmap/js/jquery.vmap.min.js"></script>
    <script src="~/Content/vendor/jqvmap/js/jquery.vmap.usa.js"></script>
    <script src="~/Content/vendor/jquery.counterup/jquery.counterup.min.js"></script>
    <script src="~/Scripts/js/dashboard/dashboard-1.js"></script>
    <!-- Ajax -->
    @Scripts.Render("~/bundles/alljs")
    <script>
        ClassicEditor
            .create(document.querySelector('#editor'))
            .catch(error => {
                console.error(error);
            });
    </script>



</body>

</html>
